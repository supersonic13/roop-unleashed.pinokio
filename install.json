{
  "run": [{
    "method": "shell.run",
    "params": {
      "message": "git clone https://github.com/C0untFloyd/roop-unleashed"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "{{os.platform === 'win32' ? 'python' : 'python3'}} -m venv env",
      "path": "roop-unleashed"
    }
  }, {
    "method": "shell.start",
    "params": {
      "path": "roop-unleashed"
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "{{os.platform() === 'win32' ? 'env\\\\Scripts\\\\activate' : 'source env/bin/activate'}}",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "pip install -r requirements.txt",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "pip uninstall onnxruntime onnxruntime-gpu",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "pip install onnxruntime-gpu==1.15.1",
      "on": [{
        "event": null,
        "return": true
      }]
    }
  }, {
    "method": "input",
    "params": {
      "title": "Install Success",
      "description": "Go back to the dashboard and launch the app!"
    }
  }]
}
